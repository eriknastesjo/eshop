<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>

<div class="content">
    <h1>Lager</h1>

    <p>Lager Lista</p>

    <table>
        <tr>
            <th>Lagerhylla</th>
            <th>Produktnamn</th>
            <th>Antal</th>
        </tr>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.lagerhylla %></td>
            <td><%= row.produktnamn %></td>
            <td> <%= row.antal %> </td>
            <td>
                <a href="/eshop/stock/edit/<%= row.produktkod %>" class="link">Edit</a>
            </td>
        </tr>
        <% }; %>
    </table>
</div>

<%- include("../partials/footer.ejs"); %>