<%- include("../partials/header.ejs", {title: title}); %>
<div class="bg"></div>
<div class="bg bg2"></div>
<div class="bg bg3"></div>

<div class="content">
    <h1>Plocklistor</h1>

    <table>
        <tr>
            <th>Ordernummer</th>
            <th>Orderdatum</th>
            <th>Kund ID</th>
            <th>Produktkod</th>
        </tr>
        <% console.log(res) %>
        <% for (const row of res) { %>
        <tr>
            <td><%= row.ordernummer %></td>
            <td><%= row.orderdatum %></td>
            <td><%= row.kund %></td>
            <td><%= row.produkt %></td>
            <td>
                <a href="/eshop/picklist/<%= row.ordernummer %> " class="link"> Edit </a>
            </td>
        </tr>
        <% }; %>
    </table>
</div>

<%- include("../partials/footer.ejs"); %>